/* Count frequency of a given character in a string */

#include <stdio.h>
int main() {
    char s[300], ch;
    int i = 0, count = 0;

    fgets(s, 300, stdin);
    scanf("%c", &ch);

    while(s[i] != '\0') {
        if(s[i] == ch)
            count++;
        i++;
    }

    printf("%d", count);

    return 0;
}
