/* Remove all vowels from a string */

#include <stdio.h>
int main() {
    char s[300];
    int i = 0;

    fgets(s, 300, stdin);

    while(s[i] != '\0') {
        char ch = s[i];
        if(ch!='a' && ch!='e' && ch!='i' && ch!='o' && ch!='u' &&
           ch!='A' && ch!='E' && ch!='I' && ch!='O' && ch!='U')
            printf("%c", ch);
        i++;
    }

    return 0;
}
