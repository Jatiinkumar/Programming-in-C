/*
Question: Rotate an array to the right by k positions.
*/

#include <stdio.h>

int main() {
    int n, i, k;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    int arr[n], rotated[n];

    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter the value of k (right rotations): ");
    scanf("%d", &k);

    // To handle k values larger than n
    k = k % n;

    // Rotate to the right by k positions
    for (i = 0; i < n; i++) {
        rotated[(i + k) % n] = arr[i];
    }

    printf("Array after right rotation:\n");
    for (i = 0; i < n; i++) {
        printf("%d ", rotated[i]);
    }

    return 0;
}
