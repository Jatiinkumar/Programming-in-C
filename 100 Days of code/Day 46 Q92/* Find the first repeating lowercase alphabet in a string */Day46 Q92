/* Find the first repeating lowercase alphabet in a string */

#include <stdio.h>
int main() {
    char s[300];
    int freq[26] = {0}, i = 0, found = 0;

    fgets(s, 300, stdin);

    while(s[i] != '\0') {
        if(s[i] >= 'a' && s[i] <= 'z') {
            int idx = s[i] - 'a';
            freq[idx]++;
            if(freq[idx] == 2) {
                printf("%c", s[i]);
                found = 1;
                break;
            }
        }
        i++;
    }

    if(!found)
        printf("No repeating character");

    return 0;
}
