/* Check if one string is a rotation of another */

#include <stdio.h>
int main() {
    char a[200], b[200], c[400];
    int i = 0, j, k, lenA = 0, lenB = 0, match = 0;

    fgets(a, 200, stdin);
    fgets(b, 200, stdin);

    while(a[lenA] != '\0') lenA++;
    while(b[lenB] != '\0') lenB++;

    if(lenA > 0 && a[lenA - 1] == '\n') lenA--;
    if(lenB > 0 && b[lenB - 1] == '\n') lenB--;

    if(lenA != lenB) {
        printf("Not Rotation");
        return 0;
    }

    for(i = 0; i < lenA; i++)
        c[i] = a[i];
    for(i = 0; i < lenA; i++)
        c[lenA + i] = a[i];
    c[2 * lenA] = '\0';

    for(i = 0; i <= 2 * lenA - lenB; i++) {
        match = 1;
        for(j = 0, k = i; j < lenB; j++, k++) {
            if(c[k] != b[j]) {
                match = 0;
                break;
            }
        }
        if(match) break;
    }

    if(match) printf("Rotation");
    else printf("Not Rotation");

    return 0;
}
