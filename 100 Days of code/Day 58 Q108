/*Write a Program to take an integer array nums and print answer[i] = product of all elements except nums[i].*/
#include <stdio.h>

int main()
{
    int n;
    scanf("%d", &n);

    int a[n], ans[n];

    for(int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    for(int i = 0; i < n; i++)
    {
        int prod = 1;
        for(int j = 0; j < n; j++)
        {
            if(i != j)
                prod *= a[j];
        }
        ans[i] = prod;
    }

    for(int i = 0; i < n; i++)
    {
        if(i == n - 1)
            printf("%d", ans[i]);
        else
            printf("%d ", ans[i]);
    }

    return 0;
}
